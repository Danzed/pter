{
  "name": "clean-ui-admin-template-angular",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --host 0.0.0.0",
    "hmr": "ng serve --configuration=hmr --open",
    "build": "ng build --prod --build-optimizer",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{ts,scss,less}\"",
    "tslint": "tslint \"src/**/*.{ts}\"",
    "stylelint": "stylelint \"src/**/*.{scss,less}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{ts,scss,less}": [
        "prettier --write",
        "git add"
      ],
      "src/**/*.{ts}": [
        "tslint"
      ],
      "src/**/*.{scss,less}": [
        "stylelint"
      ]
    },
    "ignore": [
      "src/assets/fonts/*",
      "src/assets/images/*"
    ]
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~11.0.5",
    "@angular/fire": "^5.1.1",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ngrx/effects": "^7.1.0",
    "@ngrx/router-store": "^7.1.0",
    "@ngrx/store": "^7.1.0",
    "@ngx-progressbar/core": "^5.3.1",
    "@ngx-progressbar/http": "^5.3.1",
    "@ngx-progressbar/router": "^5.3.1",
    "angular2-chartjs": "^0.5.1",
    "bootstrap-css-only": "^4.2.1",
    "c3": "0.4.22",
    "chartist": "^0.11.0",
    "chartist-plugin-tooltips-updated": "^0.0.20",
    "core-js": "^2.5.4",
    "d3": "3.5.17",
    "firebase": "^5.8.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.11",
    "ng-chartist": "^4.1.0",
    "ng-zorro-antd": "^7.0.0-rc.3",
    "ng2-nvd3": "^2.0.0",
    "ng2-rut": "^0.3.2",
    "ngx-perfect-scrollbar": "^7.2.0",
    "ngx-quill": "^4.6.3",
    "ngx-scrollbar": "^4.1.1",
    "nvd3": "^1.8.6",
    "qs": "^6.6.0",
    "quill": "^1.3.6",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "~7.2.2",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.12.18",
    "codelyzer": "~4.5.0",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.0",
    "protractor": "~5.4.0",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "~3.2.2"
  }
}
